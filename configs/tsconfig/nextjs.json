{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Next.js",
    "extends": "./base.json",
    "compilerOptions": {
        "allowJs": true,
        "incremental": true,
        "jsx": "preserve",
        "lib": ["dom", "dom.iterable", "esnext"],
        "module": "esnext",
        "noEmit": true,
        "paths": { "@/*": ["src/*"] },
        "resolveJsonModule": true,
        "target": "es5"
    }
}
